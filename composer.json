{
    "name": "becklyn/ddd-core",
    "description": "DDD framework for PHP",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Christoph Hautzinger",
            "email": "ch@beckln.com"
        },
        {
            "name": "Marko Vujnovic",
            "email": "ms@beckln.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "becklyn/utilities": "^1.0",
        "ramsey/uuid": "^4.0",
        "illuminate/collections": "^8.12",
        "webmozart/assert": "^1.9"
    },
    "require-dev": {
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^9.1"
    },
    "autoload": {
        "psr-4": {
            "Becklyn\\Ddd\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Becklyn\\Ddd\\Tests\\": "tests/"
        }
    }
}
